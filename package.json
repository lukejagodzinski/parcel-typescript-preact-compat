{
  "name": "parcel-typescript-preact-compat",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel serve src/index.html --out-dir dist --port 3000 --open",
    "build": "parcel build src/index.html --detailed-report --experimental-scope-hoisting --out-dir dist"
  },
  "devDependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "parcel-bundler": "^1.11.0",
