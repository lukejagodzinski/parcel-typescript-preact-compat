    "typescript": "^3.2.2"
  },
  "dependencies": {
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4"
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  }
}
